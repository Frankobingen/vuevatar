{
  "name": "@frnak/vuevatar",
  "version": "0.2.2",
  "main": "./dist/vuevatar.common.js",
  "license": "MIT",
  "scripts": {
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name vuevatar ./src/index.js",
    "test": "vue-cli-service serve test/index.js",
    "test-build": "vue-cli-service build test/index.js --dest=test-dist"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^4.1.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue": "^2.6.6",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "keywords": [
    "avatar",
    "generator",
    "vue",
    "vuevatar"
  ]
}
